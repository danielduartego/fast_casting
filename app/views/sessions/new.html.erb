<div class="">
  <div class="col-md-4"></div>
  <div class="col-md-4">

    <h1 class="ui header text-center">Log In</h1>
    <div class="ui form">

    <%= form_tag sessions_path do %>
      <div class="field">
        <%= label_tag :email %>
        <%= text_field_tag :email %>
      </div>
      <div class="field">
        <%= label_tag :password %>
        <%= password_field_tag :password %>
      </div>
      <%= submit_tag "Sign In", class: "positive ui button" %>
    <% end %>

    </div>
  </div>
  <div class="col-md-4"></div>
</div>

<div class="col-xs-6 col-md-3">
    <br>
    <div class="">
    <%= link_to "Create a Project", new_project_path, class: "btn btn-success btn-block" %>
    </div>
    <br>
    <div class="panel panel-default">
      <div class="panel-heading">
          <h5 class="text-center">All Projects</h5>
      </div>
        <div class="panel-body">

          <table class="table table-hover">
              <thead>
                  <tr>
                      <th>Project</th>
                      <th>Date</th>
                      <th>Status</th>
                  </tr>
              </thead>
              <tbody>
                    <% @projects.each do |proj| %>
                  <tr>
                      <td><%= link_to proj.name, proj %></td>
                      <td><%= proj.shoot_date %></td>
                      <td>()()()</td>
                    <% end %>
                  </tr>
              </tbody>
          </table>

      </div>
    </div>
</div>

  <div class="col-xs-12 col-sm-6 col-md-9 ">
<br>
